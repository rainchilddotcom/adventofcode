<Window x:Class="_12.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_12"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Margin="8">
        <StackPanel Orientation="Horizontal">
            <Button x:Name="drawNextButton" Content="Draw Next" Click="drawNextButton_Click" />
            <TextBlock x:Name="roomCount" Padding="0,8,8,8"/>
            <TextBlock x:Name="pathCount" Padding="0,8,8,8"/>
            <TextBlock x:Name="leisurelyPathCount" Padding="0,8,8,8"/>
        </StackPanel>
        <Canvas x:Name="myCanvas"/>
    </StackPanel>
</Window>
